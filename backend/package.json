{
  "name": "biometric-dao-backend",
  "version": "1.0.0",
  "description": "Backend service for biometric DAO voting system with blockchain synchronization",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "docker:build": "docker build -t biometric-dao-backend .",
    "docker:run": "docker run -p 3000:3000 biometric-dao-backend"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "ethers": "^6.8.0",
    "ws": "^8.16.0",
    "socket.io": "^4.7.4",
    "redis": "^4.6.10",
    "mongoose": "^8.0.3",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^4.0.1",
    "@noble/curves": "^1.4.0",
    "@noble/hashes": "^1.4.0",
    "noble-ciphers": "^1.0.0",
    "ipfs-http-client": "^60.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "@types/jest": "^29.5.8",
    "typescript": "^5.3.2",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "biometric",
    "dao",
    "voting",
    "blockchain",
    "homomorphic-encryption",
    "zero-knowledge",
    "backend"
  ],
  "author": "Biometric DAO Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
